{
  "name": "product-beers",
  "version": "1.0.0",
  "private": true,
  "description": "Product Beers - La comunidad de producto de Valencia",
  "scripts": {
    "dev": "npm run i18n:generate-static && next dev",
    "build": "npm run i18n:generate-static && next build",
    "prebuild": "npm run i18n:generate-static",
    "build:verbose": "next build",
    "start": "next start",
    "lint": "eslint --quiet --cache --cache-location .next/cache/eslint/ .",
    "lint:verbose": "eslint --cache --cache-location .next/cache/eslint/ .",
    "lint:fix": "eslint --fix --cache --cache-location .next/cache/eslint/ .",
    "type-check": "tsc --noEmit --pretty false",
    "type-check:verbose": "tsc --noEmit",
    "test": "vitest --run --reporter=dot",
    "test:verbose": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "check": "node scripts/pre-commit-check.js",
    "fix": "npm run lint:fix && npm run type-check",
    "generate:slice": "node scripts/generate-slice.js",
    "generate:home-section": "node scripts/generators/generate-home-section.js",
    "gen:types": "npx supabase gen types typescript --linked > src/types/database.types.ts",
    "setup": "node scripts/setup-script/index.mjs",
    "setup:legacy": "node scripts/setup.mjs",
    "setup:db": "node scripts/setup-db.mjs",
    "setup:seo": "node scripts/setup-seo.mjs",
    "setup:copy": "node scripts/setup-copy.mjs",
    "setup:verify": "node scripts/setup-script/index.mjs --verify",
    "setup:export": "node scripts/setup-script/index.mjs --export",
    "setup:reset": "node scripts/setup-script/index.mjs --reset",
    "setup:resume": "node scripts/setup-script/index.mjs --resume",
    "i18n:create-structure": "node scripts/i18n/generators/step-1-create-translation-structure.mjs",
    "i18n:generate-ai": "node scripts/i18n/generators/step-2-generate-content-with-ai.mjs",
    "i18n:sync-keys": "node scripts/i18n/generators/step-3-sync-missing-keys.mjs",
    "i18n:validate": "node scripts/i18n/validation/validate-all-translations.mjs",
    "i18n:generate-static": "node scripts/i18n/generators/generate-static-translations.mjs",
    "project:setup": "node scripts/project-setup.mjs",
    "home:generate": "node scripts/home-generator.mjs",
    "seo:generate": "node scripts/seo-generator.mjs",
    "check:links": "node scripts/check-links.mjs",
    "validate:architecture": "node scripts/validate-architecture.mjs",
    "cleanup": "node scripts/maintenance/cleanup.mjs",
    "pre-push": "npm run type-check && npm run lint && npm run test && npm run check:links && npm run validate:architecture",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "product management",
    "comunidad",
    "valencia",
    "eventos",
    "networking"
  ],
  "author": "Product Beers",
  "license": "UNLICENSED",
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@fontsource/montserrat": "^5.2.8",
    "@fontsource/poppins": "^5.2.7",
    "@fontsource/space-grotesk": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "framer-motion": "^12.23.24",
    "glob": "^13.0.0",
    "lucide-react": "^0.554.0",
    "motion": "^12.23.24",
    "next": "^16.0.3",
    "next-intl": "^4.5.3",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "linkinator": "^6.3.0",
    "vitest": "^4.0.10"
  }
}
